<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>adminServlet</servlet-name>
        <servlet-class>controller.admin.adminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>controller.common.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Statical</servlet-name>
        <servlet-class>controller.admin.Statical</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TourManagement</servlet-name>
        <servlet-class>controller.admin.TourManagement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestUploadTour</servlet-name>
        <servlet-class>controller.RequestUploadTour</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TourRequestUpload</servlet-name>
        <servlet-class>controller.staff.TourRequestUpload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminServlet</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Statical</servlet-name>
        <url-pattern>/statical</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TourManagement</servlet-name>
        <url-pattern>/tourmanagement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestUploadTour</servlet-name>
        <url-pattern>/requestuploadtour</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TourRequestUpload</servlet-name>
        <url-pattern>/tourrequestupload</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>home</welcome-file>
    </welcome-file-list>
</web-app>
